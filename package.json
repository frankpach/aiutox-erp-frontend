{
  "name": "frontend",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "dev": "react-router dev",
    "start": "react-router-serve ./build/server/index.js",
    "typecheck": "react-router typegen && tsc --noEmit",
    "typecheck:watch": "tsc --noEmit --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:strict": "eslint . --max-warnings 0",
    "format": "prettier --write \"**/*.{ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,json,css,md}\"",
    "validate": "npm run typecheck && npm run lint:strict && npm run format:check",
    "validate:fix": "npm run typecheck && npm run lint:fix && npm run format",
    "prepare": "husky install || true",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:parallel": "vitest run --threads=4",
    "test:windows": "vitest run --no-coverage --reporter=verbose",
    "test:windows:hooks": "vitest run --no-coverage --reporter=verbose app/hooks/__tests__",
    "test:windows:features": "vitest run --no-coverage --reporter=verbose app/features/**/__tests__",
    "test:windows:routes": "vitest run --no-coverage --reporter=verbose app/routes/__tests__",
    "test:windows:components": "vitest run --no-coverage --reporter=verbose app/components/**/__tests__",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:parallel": "playwright test  --project=chromium --workers=4 --headed --max-failures=1 --reporter=line",
    "test:e2e:theme": "playwright test app/__tests__/e2e/config/theme-config-e2e.spec.ts --project=chromium --workers=4 --headed --max-failures=1 --reporter=line",
    "test:theme:all": "npm run test:run -- app/hooks/__tests__/useThemeConfig.test.ts app/routes/__tests__/config.theme.test.tsx app/__tests__/integration/config/theme-api.test.ts && npm run test:e2e:theme",
    "lighthouse": "lhci autorun",
    "lighthouse:collect": "lhci collect",
    "lighthouse:upload": "lhci upload",
    "lighthouse:report": "lhci report"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@hookform/resolvers": "^3.9.1",
    "@hugeicons/core-free-icons": "^3.0.0",
    "@hugeicons/react": "^1.1.2",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-popover": "^1.1.6",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@react-router/node": "7.13.0",
    "@react-router/serve": "7.13.0",
    "@tanstack/react-query": "^5.90.12",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-syntax-highlighter": "^15.5.13",
    "ace-builds": "^1.43.5",
    "axios": "^1.7.9",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "dompurify": "^3.3.1",
    "isbot": "^5.1.31",
    "lucide-react": "^0.561.0",
    "mermaid": "^11.12.3",
    "react": "^19.2.1",
    "react-ace": "^14.0.1",
    "react-big-calendar": "^1.19.4",
    "react-dom": "^19.2.1",
    "react-hook-form": "^7.54.2",
    "react-markdown": "^10.1.0",
    "react-router": "7.13.0",
    "react-router-dom": "7.13.0",
    "react-syntax-highlighter": "^16.1.0",
    "reactflow": "^11.11.4",
    "recharts": "^3.6.0",
    "remark-gfm": "^4.0.1",
